{
  "name": "client",
  "version": "1.0.0",
  "description": "Book a meal react client",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config './webpack.config.js' --mode development --hot",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "client": "npm start",
    "server": "nodemon server.js --exec babel-node server.js --presets env",
    "starter": "concurrently --kill-others-on-fail \"cd ../ && npm start\" \"npm run client\"",
    "build": "webpack --mode production"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/_tests_/setup/setupEnzyme.js",
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/setup/"
    ],
    "testURL": "http://localhost/login",
    "moduleNameMapper": {
      "^.+\\.(css|less)$": "<rootDir>/_tests_/setup/CSSStub.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Onnassiz/Book-A-Meal.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Onnassiz/Book-A-Meal/issues"
  },
