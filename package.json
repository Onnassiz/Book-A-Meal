{
  "name": "book-a-meal",
  "version": "1.0.0",
  "description": "API for Book-A-Meal",
  "main": "index.js",
  "scripts": {
    "test": "sequelize db:migrate:undo:all --env test&& sequelize db:migrate --env test&& sequelize db:seed:all --env test&& NODE_ENV=test nyc mocha --compilers js:babel-register --recursive ./test/ --exit",
    "start": "nodemon server.js --exec babel-node server.js --presets env --ignore 'client'",
    "lint": "eslint api",
    "tests": "concurrently --kill-others-on-fail \"npm run test\" \"cd client && npm run test\"",
    "server": "npm start",
    "starter": "concurrently --kill-others-on-fail \"npm run server\" \"cd client && npm start\"",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Onnassiz/Book-A-Meal.git"
  },
  "keywords": [
    "api",
    "book-a-meal",
    "tdd"
  ],
  "author": "onnassiz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Onnassiz/Book-A-Meal/issues"
  },
  "homepage": "https://github.com/Onnassiz/Book-A-Meal#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.18.2",
    "concurrently": "^3.6.1",
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "eslint": "^4.19.1",
    "express": "^4.16.3",
    "express-validator": "^5.2.0",
    "html-loader": "^0.5.5",
    "is-url": "^1.2.4",
    "jsonwebtoken": "^8.2.1",
    "lodash": "^4.17.10",
    "moment": "^2.22.1",
    "password-hash": "^1.2.2",
    "pg": "^7.4.2",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.37.6",
    "sequelize-cli": "^4.0.0",
    "serve-favicon": "^2.5.0",
    "swagger-ui-express": "^3.0.8",
    "uuid": "^3.2.1",
    "uuid-validate": "0.0.2",
    "validate.js": "^0.12.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "coffeescript": "^2.3.0",
    "coveralls": "^3.0.0",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "mocha": "^5.1.1",
    "nodemon": "^1.17.3",
    "nyc": "^11.7.1",
    "request": "^2.85.0"
  },
  "engines": {
    "node": "6.10.3"
  },
  "eslintConfig": {
    "globals": {
      "window": true,
      "document": true
    }
  }
}
